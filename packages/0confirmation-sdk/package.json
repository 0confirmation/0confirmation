    "bluebird": "^3.5.4",
    "bufferutil": "^4.0.1",
    "chai": "^4.2.0",
    "contracts-vyper": "https://github.com/Uniswap/contracts-vyper",
    "cross-env": "^7.0.0",
    "darknode-sol": "https://github.com/renproject/darknode-sol",
    "emasm": "^0.1.10",
    "ethers": "^4.0.45",
