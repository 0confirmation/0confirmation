{
  "name": "@0confirmation/sdk",
  "version": "0.1.1",
  "description": "0confirmation sdk",
  "main": "sdk.js",
  "scripts": {
    "task:prepare": "cross-env docker-compose -f test/docker-compose.yaml build",
    "task:build:exchange": "cross-env docker-compose -f test/docker-compose.yaml run -T build-uniswap-exchange > test/build-uniswap/exchange.txt",
    "task:build:factory": "cross-env docker-compose -f test/docker-compose.yaml run -T build-uniswap-factory > test/build-uniswap/factory.txt",
    "build": "npm-run-all task:prepare task:build:*",
    "test": "mocha --exit --timeout 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0confirmation/0confirmation.git"
  },
  "keywords": [
    "0confirmation",
    "sdk"
  ],
