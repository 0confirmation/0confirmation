  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0confirmation/0confirmation/issues"
  },
  "dependencies": {
    "@nodeutils/defaults-deep": "^1.1.0",
    "@renproject/ren": "^0.7.3",
    "@truffle/workflow-compile": "^2.1.24",
