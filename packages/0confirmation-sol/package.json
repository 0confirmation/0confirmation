{
  "name": "@0confirmation/sol",
  "version": "0.1.17",
  "description": "0confirmation buidler port",
  "main": "index.js",
  "repository": "https://github.com/0confirmation/0confirmation",
  "author": "Raymond Pulver IV",
  "license": "MIT",
  "private": false,
  "scripts": {
    "task:build:mkdir": "mkdirp build",
    "task:build:clean": "rimraf build",
    "task:build:insert-uniswap": "cross-env node internal/insert-uniswap",
    "task:build:insert-curvefi": "cross-env node internal/insert-curvefi",
    "task:build:insert-uniswap-v2": "cross-env node internal/insert-uniswap-v2",
    "task:build:buidler": "buidler compile",
    "freeze": "cp build/AssetForwarder.json build/AssetForwarderFreeze.json; cp build/ShifterBorrowProxy.json build/ShifterBorrowProxyFreeze.json",
    "verify-kovan": "cross-env buidler run scripts/verify-kovan",
    "build": "npm-run-all task:build:mkdir task:build:buidler task:build:insert-uniswap-v2 task:build:insert-curvefi",
    "test": "buidler test",
    "prettier": "prettier --write ./deploy/*.js ./test/*.js scripts/*.js",
    "eslint": "cross-env eslint --fix ./deploy/*",
    "deploy": "buidler deploy"
  },
  "dependencies": {
    "@nomiclabs/buidler": "^1.3.8",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-etherscan": "^1.3.3",
    "@nomiclabs/buidler-solhint": "^1.3.3",
    "@nomiclabs/buidler-waffle": "^2.0.0",
    "buidler-deploy": "^0.4.11",
    "buidler-gas-reporter": "^0.1.3",
    "buidler-spdx-license-identifier": "^1.0.2",
    "ethereum-waffle": "^3.0.1",
    "prettier": "^2.0.5",
    "random-bytes": "^1.0.0",
    "solidity-coverage": "^0.7.9"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.0.7-beta",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@openzeppelin/contracts": "^3.1.0",
    "bip39": "^3.0.2",
    "bluebird": "^3.7.2",
    "eslint": "^7.4.0",
    "ethers": "^5.0.5",
    "local-repl": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.54",
    "random-bytes": "^1.0.0"
  },
  "repl": {
    "enableAwait": true
  },
  "gitHead": "884c6d221f130cb563986bf526955b074e81bcc5"
}
